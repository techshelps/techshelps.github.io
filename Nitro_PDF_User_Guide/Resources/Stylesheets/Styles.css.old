/*<meta />*/

body
{
	font-family: Arial;
}

h1
{
	font-family: 'Segoe UI';
	text-indent: 0;
	line-height: 35px;
	background-color: #ffffff;
	mc-leader-align: left;
	mc-leader-format: 'none';
	vertical-align: baseline;
	font-weight: normal;
	font-style: normal;
	font-size: 18pt;
	word-spacing: 3px;
	padding-left: 0px;
	border-left-style: none;
	border-left-width: 0px;
	border-left-color: #c40d5d;
	border-right-style: none;
	border-right-width: 0px;
	border-right-color: #c40d5d;
	border-top-style: none;
	border-top-width: 0px;
	border-top-color: #c40d5d;
	border-bottom-style: solid;
	border-bottom-width: 1px;
	border-bottom-color: #c40d5d;
}

h2
{
	font-weight: bold;
	font-size: 12pt;
	font-family: 'Segoe UI';
	padding-left: 5px;
	padding-right: 0px;
	border-left-style: solid;
	border-left-width: 1px;
	border-left-color: #696969;
	border-right-style: none;
	border-right-width: 0px;
	border-right-color: #dcdcdc;
}

h3
{
	font-weight: bold;
	font-family: 'Segoe UI';
	font-size: 10pt;
	border-left-style: none;
	border-left-width: 0px;
	border-left-color: #696969;
	padding-left: 0px;
}

h4
{
	font-weight: bold;
	font-size: 12.0pt;
	font-family: 'Segoe UI';
}

h5
{
	font-weight: bold;
	font-size: 10.0pt;
	font-family: 'Segoe UI';
}

h6
{
	font-weight: bold;
	font-size: 8.0pt;
	font-family: 'Segoe UI';
}

p
{
	margin-top: 0pt;
	margin-bottom: 0pt;
	font-size: 10pt;
	font-family: 'Segoe UI';
	page-break-before: auto;
	mc-hyphenate: never;
}

ol,
ul
{
	margin-top: 0px;
	margin-bottom: 0px;
}

img
{
	border: none;
}

a.Popup
{
	
}

MadCap|expandingHead
{
	font-style: italic;
	font-weight: normal;
	cursor: hand;
	text-decoration: none;
	color: #006600;
}

MadCap|expandingBody
{
	color: #777777;
	font-style: italic;
}

MadCap|dropDownHotspot
{
	cursor: hand;
	font-style: italic;
	text-decoration: none;
	color: #006600;
}

fieldset
{
	font-family: 'Segoe UI';
}

dir
{
	font-family: 'Segoe UI';
}

menu
{
	font-family: 'Segoe UI';
}

center
{
	font-family: 'Segoe UI';
}

blockquote
{
	font-family: 'Segoe UI';
}

div
{
	font-family: 'Segoe UI';
}

address
{
	font-family: 'Segoe UI';
}

td
{
	font-size: 10pt;
	font-family: 'Segoe UI';
	font-weight: normal;
	font-style: italic;
	background-color: #ffffff;
	text-align: left;
	vertical-align: bottom;
	text-indent: 5px;
	border-left-style: solid;
	border-left-width: 0pt;
	border-left-color: #BFBFBF;
	border-right-style: solid;
	border-right-width: 0px;
	border-right-color: #BFBFBF;
	border-top-style: solid;
	border-top-width: 0px;
	border-top-color: #BFBFBF;
	border-bottom-style: solid;
	border-bottom-width: 0px;
	border-bottom-color: #BFBFBF;
	padding-top: 5px;
	padding-bottom: 5px;
}

li
{
	font-size: 10pt;
	font-family: 'Segoe UI';
	font-weight: normal;
	font-style: normal;
	padding-left: 0px;
	color: #c40d5d;
}

ol
{
	font-size: 10pt;
	font-family: 'Segoe UI';
	font-weight: bold;
	font-style: normal;
	color: #000000;
}

ol p
{
	font-size: 10pt;
	font-family: 'Segoe UI';
	font-weight: normal;
	font-style: normal;
	color: #000000;
}

ul p
{
	font-size: 10pt;
	font-family: 'Segoe UI';
	font-weight: normal;
	font-style: normal;
	color: #000000;
}

ul
{
	font-size: 10pt;
	font-family: 'Segoe UI';
	font-weight: bold;
	font-style: normal;
	color: #000000;
}

table
{
	font-size: 10pt;
	font-family: 'Segoe UI';
	border-spacing: 1;
	text-align: left;
	vertical-align: bottom;
	padding-bottom: 0cm;
	border-left-style: solid;
	border-left-width: 0.5pt;
	border-left-color: #BFBFBF;
	border-right-style: solid;
	border-right-width: 0.5pt;
	border-right-color: #BFBFBF;
	border-top-style: solid;
	border-top-color: #BFBFBF;
	border-bottom-style: solid;
	border-bottom-width: 0.5pt;
	border-bottom-color: #BFBFBF;
	border-top-width: 0;
	padding-top: 0;
}

tbody
{
	font-size: 10pt;
	font-family: 'Segoe UI';
}

th
{
	text-align: left;
	vertical-align: bottom;
	text-indent: 5px;
	padding-top: 5px;
	padding-bottom: 5px;
}

thead
{
	background-color: #d9d9d9;
	text-align: left;
}

tr
{
	background-color: #f2f2f2;
	text-align: left;
	vertical-align: middle;
}

